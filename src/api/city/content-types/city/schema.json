      "target": "api::state.state",
      "inversedBy": "cities"
    },
    "zip_codes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::zip-code.zip-code",
      "mappedBy": "city"
    },
    "spots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::spot.spot",
      "mappedBy": "city"
    }
  }
}
