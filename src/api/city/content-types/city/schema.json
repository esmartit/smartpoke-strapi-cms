{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "singularName": "city",
    "pluralName": "cities",
    "displayName": "Cities",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cityCode": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "relation",
